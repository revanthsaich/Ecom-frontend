{
    "name": "ecommercefrontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "start": "electron .",
        "package": "electron-packager . ecommercefrontend --platform=win32 --arch=x64 --out=dist --overwrite"
    },
    "build": {
        "appId": "com.yourname.ecommercefrontend",
        "productName": "EcommerceFrontend",
        "win": {
            "target": "nsis"
        },
        "mac": {
            "target": "dmg"
        },
        "linux": {
            "target": "AppImage"
        }
    },
    "main": "main.js",
    "dependencies": {
        "@coreui/coreui": "^5.2.0",
        "@coreui/react": "^5.4.1",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mui/icons-material": "^6.3.0",
        "@mui/material": "^6.3.0",
        "autoprefixer": "^10.4.20",
        "axios": "^1.7.9",
        "flowbite-react": "^0.10.2",
        "postcss": "^8.4.49",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-icons": "^5.4.0",
        "react-router-dom": "^7.1.3",
        "react-transition-group": "^4.4.5",
        "styled-components": "^6.1.13",
        "tailwindcss": "^3.4.17"
    },
    "devDependencies": {
        "@eslint/js": "^9.17.0",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "@vitejs/plugin-react": "^4.3.4",
        "electron": "^33.2.1",
        "electron-builder": "^25.1.8",
        "electron-packager": "^17.1.2",
        "eslint": "^9.17.0",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.16",
        "globals": "^15.14.0",
        "vite": "^6.0.5"
    }
}
